    <Routine name="trialresult">
      <CodeComponent name="code_3">
        <Param name="Begin Experiment" val="" valType="extendedCode" updates="constant"/>
        <Param name="name" val="code_3" valType="code" updates="None"/>
        <Param name="Begin Routine" val="#--Assigns which message should be on the resulr screen&#10;if not key_press.keys:&#10;    msg='Too Late !!'&#10;elif key_press.corr:#stored on last run routine&#10;    msg='Correct'&#10;else:&#10;    msg='Incorrect'" valType="extendedCode" updates="constant"/>
        <Param name="End Routine" val="#-- Following lines of code assign the line position for the next trail&#10;chosenLocs = random.sample(possibleLocs, memSize+5)&#10;shuffle(chosenLocs)&#10;&#10;pos = []&#10;x_cords=[]&#10;y_cords=[]&#10;ind = 0&#10;&#10;for i in range(memSize):&#10;    x_cords.append((int(chosenLocs[i]%noOfCols)*barLen+int(barLen/2))-int(colSize/2))&#10;    y_cords.append((int(chosenLocs[i]/noOfCols)*barLen+int(barLen/2))-int(rowSize/2))&#10;    ind=ind+1&#10;&#10;if memSize &lt; 5 :&#10;    for i in range(5+1-memSize):&#10;        x_cords.append((int(chosenLocs[ind]%noOfCols)*barLen+int(barLen/2))-int(colSize/2))&#10;        y_cords.append((int(chosenLocs[ind]/noOfCols)*barLen+int(barLen/2))-int(rowSize/2))&#10;        ind=ind+1&#10;&#10;pos = []&#10;for i in range(len(x_cords)):&#10;    pos.append([x_cords[i],y_cords[i]])&#10;&#10;shuffle(pos)&#10;&#10;#-- Assigning the line position for the next trail complete." valType="extendedCode" updates="constant"/>
        <Param name="End Experiment" val="" valType="extendedCode" updates="constant"/>
        <Param name="Each Frame" val="" valType="extendedCode" updates="constant"/>
      </CodeComponent>
