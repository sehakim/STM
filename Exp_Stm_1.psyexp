    <Routine name="trial">
      <CodeComponent name="probStCode">
        <Param name="Begin Experiment" val="#--import the required packages.&#10;from openpyxl import load_workbook&#10;import random&#10;&#10;colSize = 800 #--MAX number of pixels along x axis&#10;rowSize = 500 #--MAX number of pixels along y_axis&#10;stmArea = [colSize, rowSize] #--Initializes the size of the rectangle in which the lines will be shown&#10;subID = expInfo['Subject_ID'] #--Gets Subject ID&#10;sessionNum = expInfo['Session_Number'] #--Gets Sessions Number&#10;memSize = int(expInfo['Memory_Set_Size']) #--Gets Memory Set Size&#10;barLen = int(expInfo['length_of_bar_px']) #--Gets length of the bar in Pixels&#10;barWidth = int(expInfo['width_of_bar_px']) #--Gets Width of the bar in Pixels&#10;dispTime = float(expInfo['length_of_display_time_ms']) #--Gets length of Display time in seconds.&#10;pauseTime = float(expInfo['length_of_pause_time_ms']) #--Gets Length of Pause time in seconds.&#10;&#10;noOfRows = int(rowSize*0.9/barLen) #-- Initialize Max number of Rows&#10;noOfCols = int(colSize*0.9/barLen) #-- Initialize Max number of Columns&#10;&#10;#--This part selects the test case number in which one of the lines should change.&#10;nRep = 12 &#10;maxRep = memSize*nRep #-- Initializes Max Number of Repetitions&#10;temp = range(maxRep) #-- Initializes Max index Numbers of Trials that can be changed&#10;chngList = random.sample(temp, int(maxRep/2)) #-- Initializes which trails numbers chould have the change of line.&#10;&#10;#--This part selects the locations for the lines&#10;noOfLocs = noOfRows*noOfCols #-- Initializes Max Number of Possible locations&#10;possibleLocs = range(noOfLocs) #-- Initializes position of all posible indexes.&#10;chosenLocs = random.sample(possibleLocs, memSize+5) #--Initializes the indexes of the positions chosen to place the line(s) at. &#10;shuffle(chosenLocs) #--Shuffles line locations.&#10;&#10;#--The following lines create the exact (x,y) positions at which the lines are to positioned.&#10;x_cords=[] #--This gives the x-coordinates&#10;y_cords=[] #--This gives the y-coordinates&#10;pos = [] #--This will give the positions in the term of (X,Y).&#10;&#10;#-- The following lines of code will assign the (X,Y) coordinates.&#10;ind=0 #-- Ind helps insure that we do not repeat location indexes while assigning X and Y coordinats below.&#10;for i in range(memSize):&#10;    #x_cords.append((int(chosenLocs[i]%noOfCols)*barLen+int(barLen/2))-int(colSize/2) + int(colSize*0.1/2) ) #--Assigning X-coordinates for given number of lines&#10;    #y_cords.append((int(chosenLocs[i]/noOfCols)*barLen+int(barLen/2))-int(rowSize/2) + int(colSize*0.1/2) ) #--Assigning Y-coordinates for given number of lines&#10;    x_cords.append( 0 ) #--Assigning X-coordinates for given number of lines&#10;    y_cords.append( 0 ) #--Assigning Y-coordinates for given number of lines&#10;&#10;    ind=ind+1&#10;&#10;#--Not adding the below buffers causes error in the lines created using buider.&#10;if memSize &lt; 5 :&#10;    for i in range(5-memSize):&#10;        #x_cords.append((int(chosenLocs[ind]%noOfCols)*barLen+int(barLen/2))-int(colSize/2) + int(colSize*0.1/2) ) #--Assigning X-coordinates for buffer positions&#10;        #y_cords.append((int(chosenLocs[ind]/noOfCols)*barLen+int(barLen/2))-int(rowSize/2) + int(colSize*0.1/2) ) #--Assigning Y-coordinates for buffer positions&#10;        x_cords.append( 20 )&#10;        y_cords.append( 20 )&#10;        ind=ind+1&#10;&#10;#--Initializes the positions for each line.&#10;pos = []&#10;for i in range(len(x_cords)):&#10;    pos.append([x_cords[i],y_cords[i]])&#10;&#10;#--This part creates the randomization of the property value assignment.&#10;shuffle(pos)&#10;&#10;for r in range(maxRep):&#10;    fcolor = ['red', 'blue', 'purple', 'yellow', 'green', 'black', 'white'] #--Standard color Values.&#10;    orientations = [0, 90, 45, -45] #--Standard orientation Values.&#10;    &#10;    #--This part randomizes the property values.&#10;    shuffle(fcolor)&#10;    shuffle(orientations)&#10;    orientations.append(random.choice(orientations))&#10;    &#10;    #--Here we right the stimuli property values to 'stimuliList.xlsx'&#10;    #--This excel file will be used as input during testing.&#10;    st = 0&#10;    while st&lt;5:&#10;        wb = load_workbook(filename = 'stimuliList.xlsx')&#10;        ws = wb.get_sheet_by_name(&quot;Sheet1&quot;)&#10;    &#10;        OO = orientations[st-1]&#10;        CC = fcolor.pop()&#10;        ind = st*2&#10;        &#10;        ws.cell(row=r+1,column=ind).value = OO #--Write orientation values to excel file&#10;        ws.cell(row=r+1,column=ind+1).value = CC  #--Write color values to excel file&#10;        &#10;        ws.cell(row=r+1,column=ind+10).value = OO #--Write orientation values to excel file&#10;        ws.cell(row=r+1,column=ind+1+10).value = CC #--Write color values to excel file&#10;        &#10;        ws.cell(row=r+1,column=20).value = 'No' #--Write changed or not to excel file&#10;        ws.cell(row=r+1,column=21).value = 's' #--Write correct key press values to excel file&#10;        ws.cell(row=r+1,column=22).value = '-'#--No line is changed&#10;        wb.save('stimuliList.xlsx')&#10;        st = st+1&#10;    #--Following lines of code assign values for the changed trails&#10;    #--Particularly this assigns the changed line color and &#10;    #--the corresponding line number is written to excel file along with the corresponding key press for this trial.&#10;    if chngList.count(r) == 1:&#10;        stmCh = random.choice(range(5))&#10;        stmCh = (stmCh*2)+1&#10;        colCh = random.choice(fcolor)&#10;        wb = load_workbook(filename = 'stimuliList.xlsx')&#10;        ws = wb.get_sheet_by_name(&quot;Sheet1&quot;)&#10;        ws.cell(row=r+1,column=stmCh+10).value = colCh #--Write changed color values to excel file&#10;        ws.cell(row=r+1,column=20).value = 'Yes' #--Indicates that a liine in the trail was chaged by writing Yes to the excel file.&#10;        ws.cell(row=r+1,column=21).value = 'd' #--Writes the corresponding correct key press to the excel file.&#10;        ws.cell(row=r+1,column=22).value = stmCh #writes which line was changed to the excel file.&#10;        wb.save('stimuliList.xlsx')" valType="extendedCode" updates="constant"/>
        <Param name="name" val="probStCode" valType="code" updates="None"/>
